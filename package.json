{
    "name": "bruter",
    "displayName": "Bruter",
    "description": "Syntax highlighting for the Bruter language",
    "version": "0.0.4",
    "publisher": "jardimdanificado",
    "engines": {
      "vscode": "^1.40.0"
    },
    "categories": ["Programming Languages"],
    "contributes": {
      "languages": [
        {
          "id": "bruter",
          "aliases": ["Bruter", "bruter"],
          "extensions": [".br"],
          "configuration": "./language-configuration.json"
        }
      ],
      "grammars": [
        {
          "language": "bruter",
          "scopeName": "source.bruter",
          "path": "./syntaxes/bruter.tmLanguage.json"
        }
      ]
    }
}
  